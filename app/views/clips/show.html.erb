 <h1 id="logo"><%= link_to "Clip.It", root_url %></h1>

  <div id="clip-show">
    <span id="clip-show-title"><strong><%= link_to "#{@clip.title}", clip_path(@clip) %></strong></span>
    <span id="clip-show-photo"><%= image_tag @clip.photo.url(:large) %></span>
    <span id="clip-show-body"><%= @clip.body %></span>
    <span id="clip-show-video"><%= raw @e_video.first %></span>
  </div>


